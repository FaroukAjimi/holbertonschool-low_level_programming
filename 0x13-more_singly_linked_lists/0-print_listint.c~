#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
size_t print_listint(const listint_t *h)
{
	unsigned int i = 0;

	if (h == NULL)
		return (0);
	while (h != NULL)
	{
		if (!h->n)
		{
			printf("0");
			h = h->next;
			i++;
			printf("\n");
		}
		printf("%d\n", h->n);
		h = h->next;
		i++;
	}
	return (i);
}
