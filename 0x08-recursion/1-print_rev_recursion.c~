#include "holberton.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
void _print_rev_recursion(char *s)
{
	int i=0;
	i++;
	if (s[i] != '\0')
	{
		_print_rev_recursion(&s[i]);
	}
	_putchar(s[i-2]);
	i--;
	if (s[i] != s[0])
	{
		_print_rev_recursion(&s[i]);
	}
}
