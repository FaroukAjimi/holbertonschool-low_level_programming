#include "holberton.h"
/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int _sqrt(int i, int n)
{

	if (i * i == n)
		return (i);
	else if (i == n)
		return (-1);
	else if (i < 0)
		return(-1);
	else
		return (_sqrt(i + 1,n));

}
int _sqrt_recursion(int n)
{
	return _sqrt(1, n);
}
